<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lading.applicationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".NormalRxActivity"/>
        <activity android:name=".RxMapActivity"/>
        <activity android:name=".RxSchuderActivity"/>
        <activity android:name=".RxFlatMapActivity"/>
        <activity android:name=".RxMergeActivity"/>
        <activity android:name=".RxBindingActivity"/>
        <activity android:name=".RxFilterActivity"/>
        <activity android:name=".RxTakeActivity"/>
        <activity android:name=".RxTimerActivity"/>
        <activity android:name=".RxSortActivity"/>
        <activity android:name=".RxConnetActivity"/>
        <activity android:name=".TimestampActivity"/>

    </application>

</manifest>