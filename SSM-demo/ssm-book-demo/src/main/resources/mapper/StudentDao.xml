<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.absentm.demo.dao.StudentDao">
	<select id="quaryStudent" resultType="com.absentm.demo.beans.Student">
		SELECT
		student_id,
		password
		FROM
		student
		WHERE
		student_id=#{studentId}
		AND password=#{password}
	</select>

</mapper>